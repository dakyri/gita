<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>Gita</servlet-name>
    <display-name>Servlet Gita</display-name>
    <description>Default configuration created for servlet.</description>
    <servlet-class>gita.Gita</servlet-class>
    <init-param>
      <param-name>project-base</param-name>
      <param-value>d:\\dak/java/projects/anoto/Gita-build/build/web/</param-value>
    </init-param>
    <init-param>
      <param-name>gita-mode</param-name>
      <param-value>hack</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Gita</servlet-name>
    <url-pattern>/servlet/gita</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>
            30
        </session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>
            index.jsp
        </welcome-file>
    <welcome-file>
            index.html
        </welcome-file>
    <welcome-file>
            index.htm
        </welcome-file>
  </welcome-file-list>
</web-app>
